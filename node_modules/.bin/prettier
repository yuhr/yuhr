#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules/@yuhr/prettier/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules/@yuhr/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/node_modules:/Users/yuhr/Workspace/yuhr/node_modules:/Users/yuhr/Workspace/node_modules:/Users/yuhr/node_modules:/Users/node_modules:/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/prettier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules/@yuhr/prettier/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules/@yuhr/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/@yuhr+prettier@2.3.2/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/.pnpm/node_modules:/Users/yuhr/Workspace/yuhr/node_modules:/Users/yuhr/Workspace/node_modules:/Users/yuhr/node_modules:/Users/node_modules:/node_modules:/Users/yuhr/Workspace/yuhr/node_modules/prettier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
